msgid "plugins.themes.defaultOjs.name"
msgstr "Default OJS Theme"

msgid "plugins.themes.defaultOjs.description"
msgstr "This theme implements the default OJS theme."

msgid "plugins.themes.defaultOjs.option.typography.label"
msgstr "Typography"

msgid "plugins.themes.defaultOjs.option.typography.description"
msgstr "Choose a font combination that suits this journal."

msgid "plugins.themes.defaultOjs.option.typography.notoSans"
msgstr ""
"Noto Sans: A digital-native font designed by Google for extensive language "
"support."

msgid "plugins.themes.defaultOjs.option.typography.notoSerif"
msgstr "Noto Serif: A serif variant of Google's digital-native font."

msgid "plugins.themes.defaultOjs.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: A complementary pairing with serif headings and sans-"
"serif body text."

msgid "plugins.themes.defaultOjs.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: A complementary pairing with sans-serif headings and "
"serif body text."

msgid "plugins.themes.defaultOjs.option.typography.lato"
msgstr "Lato: A popular modern sans-serif font."

msgid "plugins.themes.defaultOjs.option.typography.lora"
msgstr "Lora: A wide-set serif font good for reading online."

msgid "plugins.themes.defaultOjs.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: A complimentary pairing with serif headings and sans-serif "
"body text."

msgid "plugins.themes.defaultOjs.option.colour.label"
msgstr "Colour"

msgid "plugins.themes.defaultOjs.option.colour.description"
msgstr "Choose a colour for the header."

msgid "plugins.themes.defaultOjs.option.showDescriptionInJournalIndex.option"
msgstr "Show the journal summary on the homepage."

msgid "plugins.themes.defaultOjs.option.useHomepageImageAsHeader.label"
msgstr "Header Background Image"

msgid "plugins.themes.defaultOjs.option.useHomepageImageAsHeader.description"
msgstr ""
"When a homepage image has been uploaded, display it in the background of the "
"header instead of it's usual position on the homepage."

msgid "plugins.themes.defaultOjs.option.useHomepageImageAsHeader.option"
msgstr "Show the homepage image as the header background."

msgid "plugins.themes.defaultOjs.option.displayStats.label"
msgstr "Usage statistics display options"

msgid "plugins.themes.defaultOjs.option.displayStats.none"
msgstr "Do not display submission usage statistics chart for reader."

msgid "plugins.themes.defaultOjs.option.displayStats.bar"
msgstr "Use bar type of the chart for usage statistics display."

msgid "plugins.themes.defaultOjs.option.displayStats.line"
msgstr "Use line type of the chart for usage statistics display."

msgid "plugins.themes.defaultOjs.displayStats.monthInitials"
msgstr "Jan Feb Mar Apr May Jun Jul Aug Sep Oct Nov Dec"

msgid "plugins.themes.defaultOjs.displayStats.downloads"
msgstr "Downloads"

msgid "plugins.themes.defaultOjs.displayStats.noStats"
msgstr "Download data is not yet available."
